{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/order": ["error", { "newlines-between": "always" }],
    "no-console": ["error", { "allow": ["info", "warn", "error"] }],
    "spaced-comment": ["error", "always", { "markers": ["/"] }]
  }
}
